/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And (a=a,b=b, out=aandb);
    Not (in=aandb, out=notaandb);
    Or (a=a, b=b, out=aorb);
    And (a=aorb, b=notaandb, out=out);

}
